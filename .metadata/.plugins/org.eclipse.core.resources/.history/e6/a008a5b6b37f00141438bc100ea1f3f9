var win1 = Ti.UI.createWindow({
	backgroundGradient: {
        type: 'linear',
        startPoint: { x: 50, y: 50 },
        endPoint: { x: 50, y: 50 },
        colors: [ '#0000FF', '#676CFB'],
        startRadius: '90%',
        endRadius: 0,
        backfillStart: true
    }
});

var navWin = Ti.UI.i0S.createNavigationWindow({
	window: win1
});

navWin.open();
