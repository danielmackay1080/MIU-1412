var bgImage = ['rectangle_1.png'];

var win1 = Ti.UI.createWindow({
	backgroundImage: 'backgroundimage/'+bgImage
});

var nav = Ti.UI.iOS.createNavigationWindow({
	window: win1
});

var view1 = Ti.UI.createView({
	top: 200,
	height: 400,
	left:10,
	right:10,
	borderRadius: 12
});

nav.open();
win1.add(view1);

